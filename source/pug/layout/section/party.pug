-const party = [{src: "img/photo-devid.jpg", clarification: "классика, нуар", srcAuthor: "img/devid.png", name: "Дэвид Робинсон", land: "США"}, {src: "img/photo-kelli.jpg", clarification: "авангард, имрессионизм", srcAuthor: "img/kelly.png", name: "Келли Макдональд", land: "ИРЛАНДИЯ"}, {src: "img/photo-kostasi.jpg", clarification: "эклектика, экотренд", srcAuthor: "img/kostas.png", name: "Костас Папаниколау", land: "Греция"}]
-const socialParty = [{name: "insta", width: "18", height: "18", alt: "Мы в Инстаграме"}, {name: "fb", width: "9", height: "18", alt: "Мы в Фейсбуке"}, {name: "twitter", width: "19", height: "15", alt: "Мы в Твиттере"}]

section.pary
  h2.party__title Участники
    .party__container.swiper-container
      .party__catalog.swiper-wrapper
        for i in party
          .party__item.swiper-slide
            img.party__img(src=i.src, alt="Photo")
            span.party__clarification=i.clarification
            .party__wrapper-author
              .party__img-mask
                img.party__img-author(src=i.srcAuthor, alt="Photo author")
              p.party__name-author=i.name
              p.party__author-land=i.land
              ul.party__social
                for i in socialParty
                  li.promo__item
                    a.promo__social-link(href="#")
                      svg.promo__social-svg(role="img" aria-label=i.alt width=i.width height=i.height)
                        use(xlink:href="img/stack.svg#" +i.name)
    .party__arrows
      .party__button.party__button--next
        svg.party__svg.party__svg--next(role="img" width="43" height="80")
          use(xlink:href="img/stack.svg#arrow")
      .party__button.party__button--prev
        svg.party__svg.party__svg--prev(role="img" width="43" height="80")
          use(xlink:href="img/stack.svg#arrow")

